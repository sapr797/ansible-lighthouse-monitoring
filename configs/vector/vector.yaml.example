sources:
  lighthouse_logs:
    type: file
    include: ["/var/log/lighthouse/audit.log"]
    ignore_older_secs: 600
    read_from: "beginning"

sinks:
  clickhouse_sink:
    type: clickhouse
    inputs: ["lighthouse_logs"]
    endpoint: "http://127.0.0.1:8123"
    database: "lighthouse_metrics"
    table: "audits"
    skip_unknown_fields: true
    format: "json_each_row"
